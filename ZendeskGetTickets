from urllib.parse import urlencode

import requests

credentials = 'rderobertis@zendesk.com', '@Password1'
session = requests.Session()
session.auth = credentials

params = {
    'query': 'type:user role:admin', #Where to edit the search params for the call, what object and other criteria#
    'sort_by': 'created_at',
    'sort_order': 'asc'
    }

url = 'https://z3nblades.zendesk.com/api/v2/search.json?' + urlencode(params)
response = session.get(url)
if response.status_code != 200:
    print('Status:', response.status_code, 'Problem with this request. Exiting.')
    exit()


#Print the subject of each user in the results#

data = response.json()
for result in data['results']:
    print(result['name'])
    print(result['email'])
